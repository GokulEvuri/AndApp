<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="70dp" >
    
    <TextView 
        android:id="@+id/res_name"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:gravity="left"
        android:padding="5dp"
        android:textSize="15sp"
        android:text="Name"
        />
    
    <TextView 
        android:id="@+id/res_distance"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:gravity="left"
        android:padding="5dp"
        android:textSize="15sp"
        android:text="1km"
        android:layout_toRightOf="@+id/res_store_name"
        />
    
    <TextView 
        android:id="@+id/res_store_name"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:gravity="left"
        android:padding="5dp"
        android:textSize="15sp"
        android:text="Store"
        android:layout_toRightOf="@+id/res_name"
        />
    
    <TextView 
        android:id="@+id/res_price"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:gravity="left"
        android:padding="5dp"
        android:textSize="15sp"
        android:text="0 SEK"
        android:layout_below="@+id/res_name"
        
        />
    
    <TextView 
        android:id="@+id/res_timings"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:gravity="left"
        android:padding="5dp"
        android:textSize="15sp"
        android:text="00:00-00:00"
        android:layout_toRightOf="@+id/res_price"
        android:layout_below="@+id/res_name"
        />
    
    <TextView 
        android:id="@+id/res_phone"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:gravity="left"
        android:padding="5dp"
        android:textSize="15sp"
        android:text="0000000000"
        android:layout_toRightOf="@+id/res_timings"
        android:layout_below="@+id/res_name"
        />
    
</RelativeLayout>
